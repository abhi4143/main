#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
void fun(string str) { 
     
        int i, length;
        int flag = 0;   
        length = str.size();
  

        for(i=0;i < length ;i++)
        {
                if(str[i] != str[length-i-1])
                {
                        flag = 1;
                        break;
                }
        }    
        if(flag)
        {
                cout<<"No"<<endl;
        }    
        else
        {
                cout<<"Yes"<<endl;
        } 
} 

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n;
    cin>>n;
    while(n!=0)
    {
        int m;
        cin>>m;
        string a;
        char b;
        while(m!=0)
        {
            cin>>b;
            a=a+b;
            
            m--;
        }
        fun(a); 
        n--;
    }
    return 0;
}
